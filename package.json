{
  "name": "yukitoki",
  "description": "A customizable website to view docs for your projects.",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "build": "routify -b && rollup -c",
    "dev": "run-p dev:*",
    "dev:rollup": "rollup -c -w",
    "dev:routify": "routify",
    "start": "sirv public --single --no-clear",
    "format": "prettier --write config.js src/*.{js,json,ts,html,svelte} src/**/*.{js,json,ts,html,svelte}"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@roxi/routify": "^2.18.3",
    "autoprefixer": "^10.3.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.4.0",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup": "^2.56.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.42.5",
    "svelte-check": "^2.2.5",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^2.2.15"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "common-tags": "^1.8.0",
    "fontawesome-svelte": "^2.0.1",
    "fuse.js": "^6.4.6",
    "highlight.js": "^11.2.0",
    "lodash": "^4.17.21",
    "marked": "^3.0.3",
    "semver": "^7.3.5",
    "sirv-cli": "^1.0.14",
    "svelte-switch": "0.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevSnowflake/yukitoki.git"
  },
  "keywords": [
    "yukitoki",
    "docs",
    "site"
  ],
  "author": "DevAndromeda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DevSnowflake/yukitoki/issues"
  },
  "homepage": "https://github.com/DevSnowflake/yukitoki#readme"
}
